    "dev:staging": "cross-env NODE_ENV=staging npm run build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/index.ts\"",
    "dev:production": "cross-env NODE_ENV=production npm run build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/index.ts\""
